{
    "extends": [
      "airbnb-base",
      "eslint:recommended",
      "prettier",
      "plugin:node/recommended"
    ],
    "env":{
      "mocha":true
    },
    "plugins": ["prettier", "mocha", "chai"],
    "rules": {
      "prettier/prettier": "error",
      "no-unused-vars": "warn",
      "no-console": "off",
      "semi":"off",
      "no-param-reassign": 0,
      "no-useless-constructor": 0,
      "no-else-return": 0,
      "node/no-unsupported-features/es-syntax":0,
      "no-underscore-dangle":0,
      "prefer-destructuring":0,
      "consistent-return":0
    }
  }